var setTime = function (data) {
 if(data!=undefined){
  var fen = ((data / 1000 / 60) + '').split('.')[0] <= 9 ? '0' + ((data / 1000 / 60) + '').split('.')[0] : ((data / 1000 / 60) + '').split('.')[0]
  var miao = ((data / 1000 / 60) + '').split('.')[1].substring(0, 2)
  return fen + ':' + miao
 }
}
module.exports = {
  setTime:setTime
}